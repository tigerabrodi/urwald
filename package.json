    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^3.1.2",
    "happy-dom": "^17.4.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
