{
  "name": "urwald",
  "version": "0.0.1",
  "description": "A tiny, type-safe UI library for modern browsers",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
